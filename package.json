{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -rf dist && npm cache clean --force && rm -rf .vercel",
    "cleanup": "npx modclean --run",
    "typecheck": "tsc --noEmit",
    "directoryclean": "find ./src -type d -empty -delete"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-is": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3"
  }
}
